# Campo360 Manager - Frontend Configuration Template
# Copia este archivo como .env.local y configura los valores apropiados

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456789012

# Application Configuration
VITE_APP_NAME="Campo360 Manager"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Sistema integral de gestión agrícola y ganadera"

# Environment
VITE_NODE_ENV="development"

# Debug Mode
VITE_DEBUG=true

# PWA Configuration
VITE_PWA_ENABLED=true

# Authentication
VITE_TOKEN_STORAGE_KEY="campo360_token"
VITE_TOKEN_EXPIRES_DAYS=7

# UI Configuration
VITE_DEFAULT_THEME="light"
VITE_SIDEBAR_COLLAPSED=false
